{
	"name": "obsidian-gospel-study",
	"version": "0.8.0",
	"description": "An Obsidian plugin that allows for importing scripture verses or other quotes into an Obsidian note by simply pasting in the Gospel Library app share link (i.e. of the format \"https://www.churchofjesuschrist.org/study/*\")",
	"main": "main.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/kkartch0/obsidian-gospel-study"
	},
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node release.mjs",
		"test": "jest"
	},
	"keywords": [],
	"author": {
		"name": "Ky Kartchner",
		"url": "https://github.com/kkartch0"
	},
	"license": "MIT",
	"devDependencies": {
		"@types/jest": "^29.5.12",
		"@typescript-eslint/eslint-plugin": "8.31.1",
		"@typescript-eslint/parser": "8.31.1",
		"builtin-modules": "5.0.0",
		"esbuild": "0.25.3",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"node-fetch": "^2.7.0",
		"obsidian": "latest",
		"ts-jest": "^29.1.2",
		"tslib": "2.8.1",
		"typescript": "5.8.3",
		"yaml": "^2.4.1"
	}
}
