{
	"name": "obsidian-gospel-study",
	"version": "1.0.0",
	"description": "An Obsidian plugin that allows for importing scripture verses or other quotes into an Obsidian note by simply pasting in the Gospel Library app share link (i.e. of the format \"https://www.churchofjesuschrist.org/study/*\")",
	"main": "main.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/kkartch0/obsidian-gospel-study"
	},
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "jest"
	},
	"keywords": [],
	"author": {
		"name": "Ky Kartchner",
		"url": "https://github.com/kkartch0"
	},
	"license": "MIT",
	"devDependencies": {
		"@types/jest": "^29.5.12",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"obsidian": "latest",
		"ts-jest": "^29.1.2",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
